{
  "name": "vue-color-detect",
  "version": "1.0.0",
  "module": "./dist/vue-color-detect.min.js",
  "description": "A simple hook for detecting the color scheme a users browser is in",
  "main": "index.js",
  "type": "module",
  "types": "./types/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
